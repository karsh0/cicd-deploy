{
  "name": "cicd-deploy",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "start:frontend":"npm run dev",
    "start:backend":"cd ./apps/backend && npx tsc -b && node ./dist/index.js",
    "start:websocket":"cd ./apps/websocket && npx tsc -b && node ./dist/index.js",
    "db:generate":"cd ./packages/db && npx prisma generate && cd ../.."
  },
  "devDependencies": {
    "prettier": "^3.6.0",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "ws": "^1.0.0"
  }
}
